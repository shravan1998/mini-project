<!DOCTYPE html>
<html>
  <head>
    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!--Import materialize.css-->
    <link type="text/css" rel="stylesheet" href="node_modules/materialize-css/dist/css/materialize.min.css"  media="screen,projection"/>
    
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <style>
      @import url('https://fonts.googleapis.com/css?family=Slabo+27px');
      </style> 
    <title>Railway ticket booking</title>
    
  </head>

  <body>
      <nav class="light-blue">
          <div class="nav-wrapper">
              <a class="brand-logo">Railway Ticket Booking</a>
              <ul id="nav-mobile" class="right hide-on-med-and-down">
                
                <li><a href="login">Login</a></li>
          </div>
        </nav>
        <div>
          
            <% train.forEach(function(x){ %>
  
            <form>
               <div>Train Name:<%= x.TNAME %></div> 
               <div>Source:<%= x.SOURCE %></div>
               <div>Destination:<%= x.DEST %></div>
               <div>Train Number:<%= x.TNUM %></div> 
               <div>Number Of Seats:<%= x.SEATS %></div> 
              <input type="submit" class="waves-effect waves-light  btn" name="Delete" value="Delete">
            </form>
            <% }); %> 
        </div>
    
        

              

    <!--JavaScript at end of body for optimized loading-->
    <script type="text/javascript" src="node_modules/materialize-css/dist/js/materialize.min.js"></script>
   
  </body>
</html>